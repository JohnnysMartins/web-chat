{
  "name": "web-chat",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc",
    "server": "node build/index.js",
    "watch:build": "tsc --watch",
    "watch:server": "nodemon './build/index.js' --watch './build'",
    "dev": "ts-node src/index.ts",
    "start": "npm-run-all clean build --parallel watch:build watch:server --print-label",
    "prod": "npm-run-all clean build --parallel server",
    "test": "tsc && mocha build/**/*.spec.js"
  },
  "keywords": [],
  "author": "Johnnys Martins",
  "license": "ISC",
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "odi": "^0.3.4",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^2.2.0",
    "ts-node": "^8.0.2"
  },
  "devDependencies": {
    "@types/react": "^16.7.22"
  }
}
